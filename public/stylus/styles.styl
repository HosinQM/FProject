googleColor = rgb(247, 43, 28)
twitterColor = rgb(0, 153, 255)

svg.default
    width: 50px
    height: 50px
svg.white
    fill: white
section button
    margin-bottom: 0.5rem
.brands_color-google
    background-color: googleColor
    color: white
    border: none
    &:hover
        color: white
        background-color: darken(googleColor, 10)
        border: none
.brands_color-twitter
    background-color: twitterColor
    color: white
    border: none
    &:hover
        color: white
        background-color: darken(twitterColor, 10)
        border: none